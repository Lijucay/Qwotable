<resources>

    <style name="AppThemeDynamic" parent="Theme.Material3.Dark.NoActionBar">

        <item name="android:windowLightStatusBar">true</item>
        <item name="android:statusBarColor">@android:color/transparent</item>
        <item name="android:navigationBarColor">@android:color/transparent</item>

        <item name="preferenceTheme">@style/PreferenceThemeOverlay.Rikka.Material3</item>
        <item name="android:windowActivityTransitions">true</item>
    </style>

    <style name="AppTheme" parent="Theme.Material3.Light.NoActionBar">
        <item name="colorPrimary">#00629D</item>
        <item name="colorOnPrimary">#FFFFFF</item>
        <item name="colorPrimaryContainer">#CFE5FF</item>
        <item name="colorOnPrimaryContainer">#001D33</item>
        <item name="colorSecondary">#526070</item>
        <item name="colorOnSecondary">#FFFFFF</item>
        <item name="colorSecondaryContainer">#D5E4F7</item>
        <item name="colorOnSecondaryContainer">#0E1D2A</item>
        <item name="colorTertiary">#006782</item>
        <item name="colorOnTertiary">#FFFFFF</item>
        <item name="colorTertiaryContainer">#BBE9FF</item>
        <item name="colorOnTertiaryContainer">#001F29</item>
        <item name="colorError">@color/md_theme_light_error</item>
        <item name="colorErrorContainer">@color/md_theme_light_errorContainer</item>
        <item name="colorOnError">@color/md_theme_light_onError</item>
        <item name="colorOnErrorContainer">@color/md_theme_light_onErrorContainer</item>
        <item name="android:colorBackground">#FCFCFF</item>
        <item name="colorOnBackground">#1A1C1E</item>
        <item name="colorSurface">#FCFCFF</item>
        <item name="colorOnSurface">#1A1C1E</item>
        <item name="colorSurfaceVariant">#DEE3EB</item>
        <item name="colorOnSurfaceVariant">#42474E</item>
        <item name="colorOutline">#72777F</item>
        <item name="colorOnSurfaceInverse">#F1F0F4</item>
        <item name="colorSurfaceInverse">#2F3033</item>
        <item name="colorPrimaryInverse">#99CBFF</item>

        <item name="android:windowLightStatusBar">true</item>
        <item name="android:statusBarColor">@android:color/transparent</item>
        <item name="android:navigationBarColor">@android:color/transparent</item>

        <item name="preferenceTheme">@style/PreferenceThemeOverlay.Rikka.Material3</item>
        <item name="android:windowActivityTransitions">true</item>

    </style>

    <style name="ActivityAnim" parent="@android:style/Animation.Activity">
        <item name="android:activityOpenEnterAnimation">@anim/fade_in</item>
        <item name="android:activityCloseEnterAnimation">@anim/fade_in</item>
        <item name="android:activityCloseExitAnimation">@anim/fade_out</item>
        <item name="android:activityOpenExitAnimation">@anim/fade_out</item>
    </style>

    <style name="DialogAnimation">
        <item name="android:windowEnterAnimation">@anim/fade_in</item>
        <item name="android:windowExitAnimation">@anim/fade_out</item>
    </style>

    <style name="Theme.App.Starting" parent="Theme.SplashScreen">
        <!-- Set the splash screen background, animated icon, and animation duration. -->
        <item name="windowSplashScreenBackground">#ffffff</item>

        <!-- Use windowSplashScreenAnimatedIcon to add either a drawable or an
             animated drawable. One of these is required. -->
        <item name="windowSplashScreenAnimatedIcon">@drawable/ic_launcher_foreground</item>
        <!-- Required for animated icons -->
        <item name="windowSplashScreenAnimationDuration">500</item>

        <!-- Set the theme of the Activity that directly follows your splash screen. -->
        <!-- Required -->
        <item name="postSplashScreenTheme">@style/AppTheme</item>
    </style>

