        <item name="colorOnPrimary">#FFFFFF</item>
        <item name="colorPrimaryContainer">#8DFC6F</item>
        <item name="colorOnPrimaryContainer">#022100</item>
        <item name="colorSecondary">#006E25</item>
        <item name="colorOnSecondary">#FFFFFF</item>
        <item name="colorSecondaryContainer">#77FE87</item>
        <item name="colorOnSecondaryContainer">#002106</item>
        <item name="colorTertiary">#006E18</item>
        <item name="colorOnTertiary">#FFFFFF</item>
        <item name="colorTertiaryContainer">#88FC82</item>
        <item name="colorOnTertiaryContainer">#002203</item>
